{{#if_not_empty enum}}
    {{#if_not_empty name}}
{{uppercase_first_letter name}}
    {{/if_not_empty }}    
{{/if_not_empty }}

{{#if_equals type 'string'}}
    {{#with_matching format 'date' 'DateTime' }}
{{.}}
    {{else}}
        {{#if_not_empty enum}}
{{uppercase_first_letter @../@key}}
        {{else}}
string
        {{/if_not_empty }}    
    {{/with_matching}}
{{/if_equals}}

{{#if_equals type 'integer'}}
    {{#with_matching format 'int32' 'int' 'int64' 'long'}}
{{.}}
    {{else}}
int
    {{/with_matching}}
{{/if_equals}}

{{#if_equals type 'boolean'}}
bool
{{/if_equals}}

{{#if_equals type 'file'}}
Stream
{{/if_equals}}

{{#if_equals type 'object'}}
    {{#if_not_empty additionalProperties}}
        {{#if_equals additionalProperties.type 'object'}}
        
        {{else}}
            IEnumerable<(string,{{> types additionalProperties}})>
        {{/if_equals}}
    {{/if_not_empty }}  
{{/if_equals}}